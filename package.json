{
  "name": "tokensniffer-widgets",
  "version": "0.0.1",
  "description": "React components to render results from TokenSniffer API",
  "author": "Solidus Labs",
  "license": "MIT",
  "repository": "https://github.com/SolidusLabsExternal/tokensniffer-widgets",
  "scripts": {
    "build": "rm -rf dist && NODE_ENV=production babel src --out-dir dist --copy-files"
  },
  "keywords": [
    "tokensniffer",
    "web3",
    "defi"
  ],
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "babel": {
    "presets": [
        [
            "@babel/env",
            {
                "targets": {
                    "edge": "17",
                    "firefox": "60",
                    "chrome": "67",
                    "safari": "11.1"
                },
                "useBuiltIns": "usage",
                "corejs": "3.6.5"
            }
        ],
        "@babel/preset-react"
    ],
    "plugins": [
        [
            "inline-react-svg",
            {
                "svgo": false
            }
        ]
    ]
  },
  "dependencies": {
    "@babel/polyfill": "^7.12.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.22.10",
    "@babel/core": "^7.22.11",
    "@babel/preset-env": "^7.22.10",
    "babel-cli": "^6.26.0",
    "babel-plugin-inline-react-svg": "^2.0.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "^5.0.1"
  }
}
